  <% contact.messages.each do |m| %>
  <div class="individual-message-div">
    <div class='message_date'><%= Date.strptime(m.time_stamp.to_s, '%s') %></div><br>
    <div class='individual_message' data='<%=m.id%>'><%= m.snippet %></div>
<!-- m.summary.gsub(/[^0-9a-zA-Z .]/, ' ') -->
    <% if m.body_html == nil %>
      <div class='individual_message_outer_div' id='<%=m.id%>' style='display:none'><div class='individual_message_div'><%= m.body_plain_text.to_s.html_safe%></div></div>
    <% else %>
    <!-- <div class='individual_message' data='<%=m.id%>'><%= m.summary %></div> -->
      <div class='individual_message_outer_div' id='<%=m.id%>' style='display:none'><div class='individual_message_div'><%= Nokogiri::HTML(m.body_html).at('body').to_s.html_safe %></div></div>
    <% end %>
  </div>
  <% end %>
