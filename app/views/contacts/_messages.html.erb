  <% contact.messages.each do |m| %>
  <div class="individual-message-div">
    <div class='message_date'><%= Date.strptime(m.time_stamp.to_s, '%s') %></div><br>
    <div class='individual_message' data='<%=m.id%>'><%=m.summary.gsub(/[^0-9a-zA-Z .]/, ' ')%></div>
    <div class='individual_message_outer_div' id='<%=m.id%>' style='display:none'><div class='individual_message_div'><%= m.body_plain_text.gsub(/[^0-9a-zA-Z .]/, ' ') %></div></div>
  </div>
  <% end %>
